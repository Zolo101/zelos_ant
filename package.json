        "vite": "^7.2.2"
    },
    "type": "module",
    "dependencies": {
        "@blockly/continuous-toolbox": "^7.0.3",
        "@blockly/field-colour-hsv-sliders": "^6.0.6",
        "@ffmpeg/ffmpeg": "^0.12.15",
        "@ffmpeg/util": "^0.12.2",
        "blockly": "^12.3.1",
        "pocketbase": "^0.26.3",
        "twgl.js": "^7.0.0",
        "webgl-lint": "^1.11.4"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    }
}
