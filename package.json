{
	"name": "zelosant",
	"version": "1.2.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig tsconfig.json",
		"check:watch": "svelte-check --tsconfig tsconfig.json --watch",
		"asbuild:debug": "asc src/ant/assembly/index.ts --target debug",
		"asbuild:release": "asc src/ant/assembly/index.ts --target release",
		"asbuild": "npm run asbuild:debug && npm run asbuild:release"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.39",
		"@sveltejs/kit": "^1.0.0-next.415",
		"@types/ramda": "^0.28.15",
		"assemblyscript": "^0.20.18",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"blockly": "^8.0.5",
		"dexie": "^3.2.2",
		"ramda": "^0.28.0"
	}
}
